    <view id="molnetenhancer" label="Enhance with MolNetEnhancer" group="Advanced Views - Experimental Views">
        <blockRef id="main" type="molnetenhancer"/>
    </view>
    <block id="molnetenhancer" type="dummylinkout">
        <data>
