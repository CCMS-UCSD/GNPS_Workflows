		<parameter name="spec_on_server" label="Spectrum Files (Required)">
			<validator type="required"/>
			<fileGenerator type="upload" purpose="spectrum" target="spec"/>
		</parameter>

		<parameter name="spec_on_server_group2" label="Spectrum Files G2">
